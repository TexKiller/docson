  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TexKiller/node-docson"
  },
  "keywords": [
    "json",
    "schema",
    "documentation",
    "node"
  ],
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
