{
  "name": "node-docson",
  "version": "0.4.0",
  "description": "Documentation for your JSON types on node",
  "main": "index.js",
  "browser": "node-docson.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "test": "cd tests && node test.js && cd ..",
    "test-browserify": "browserify tests/test-browser.js -o tests/test-browserify-bundle.js",
    "test-webpack": "./node_modules/.bin/webpack tests/test-browser.js tests/test-webpack-bundle.js",
    "prepare": "require-self"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TexKiller/node-docson"
  },
  "keywords": [
    "json",
    "schema",
    "documentation",
    "node"
  ],
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "author": "Faister Cabrera Carvalho",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TexKiller/node-docson/issues"
  },
  "homepage": "https://github.com/TexKiller/node-docson#readme",
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^14.4.0",
    "jquery": "^2.2.2",
    "require-self": "^0.1.0",
    "transform-loader": "^0.2.4",
    "webpack": "^3.1.0"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "highlight.js": "^9.6.0",
    "jsdom": "^11.0.0",
    "jsonpointer.js": "^0.4.0",
    "marked": "^0.3.5",
    "sync-request": "^4.1.0",
    "traverse": "^0.6.6"
  }
}
