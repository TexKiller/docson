  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lbovet/docson/issues"
  },
  "homepage": "https://github.com/lbovet/docson#readme",
  "devDependencies": {
    "brfs": "^1.4.3",
    "require-self": "^0.1.0"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "highlight.js": "^9.6.0",
    "jquery": "^2.2.2",
    "jsdom": "^11.0.0",
    "jsonpointer.js": "^0.4.0",
    "marked": "^0.3.5",
    "sync-request": "^4.1.0",
    "traverse": "^0.6.6"
  }
