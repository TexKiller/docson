{
  "name": "node-docson",
  "version": "0.1.0",
  "description": "Documentation for your JSON types on node",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "test": "cd tests && node test.js && cd ..",
